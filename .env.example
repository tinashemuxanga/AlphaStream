# AlphaStream Environment Configuration
# Copy this file to .env and fill in values.

# Experiment Tracking
WANDB_API_KEY=
WANDB_PROJECT=AlphaStream
WANDB_ENTITY=
WANDB_MODE=online  # or offline

# Data and Storage
DATA_DIR=./data
CACHE_DIR=./data/cache

# APIs and Keys (optional)
ALPHA_VANTAGE_API_KEY=
POLYGON_API_KEY=
IEX_CLOUD_API_KEY=
FINNHUB_API_KEY=

# Database / Cache / Streaming
DATABASE_URL=postgresql+asyncpg://alphastream:alphastream@localhost:5432/alphastream
REDIS_URL=redis://localhost:6379
KAFKA_BROKERS=localhost:9092

# Training Defaults
EXPERIMENT_NAME=baseline
SEED=42
N_FOLDS=5

# Inference API
API_HOST=0.0.0.0
API_PORT=8080

